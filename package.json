    "classnames": "^2.2.6",
    "dva": "^2.4.1",
    "path-to-regexp": "^6.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.6",
    "umi": "^3.2.9",
    "umi-request": "^1.3.5"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.22",
    "@types/classnames": "^2.2.10",
