{
  "name": "hera",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "start:no-mock": "cross-env MOCK=none REACT_APP_ENV=dev umi dev",
    "start": "cross-env UMI_UI=none umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi test"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.0.6",
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-layout": "^6.5.14",
    "@ant-design/pro-table": "^2.9.3",
    "antd": "^4.8.2",
    "classnames": "^2.2.6",
    "dva": "^2.6.0-beta.20",
    "moment": "^2.29.1",
    "path-to-regexp": "^6.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "umi": "^3.2.26",
    "umi-request": "^1.3.5"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.23",
    "@types/classnames": "^2.2.11",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.15",
    "@types/qs": "^6.9.5",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@umijs/fabric": "^2.3.1",
    "@umijs/plugin-blocks": "^2.2.1",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-react": "^1.6.19",
    "carlo": "^0.9.46",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.13.0",
    "jsdom-global": "^3.0.2",
    "mockjs": "^1.1.0",
    "prettier": "^2.1.2",
    "puppeteer-core": "^5.4.1",
    "stylelint": "^13.7.2",
    "typescript": "^4.0.5"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
