    "prettier": "^2.1.1",
    "puppeteer-core": "^5.2.1",
    "stylelint": "^13.7.0",
    "typescript": "^4.0.2"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
