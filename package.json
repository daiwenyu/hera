    "classnames": "^2.2.6",
    "dva": "^2.4.1",
    "moment": "^2.27.0",
    "path-to-regexp": "^6.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "umi": "^3.2.19",
    "umi-request": "^1.3.5"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.22",
    "@types/classnames": "^2.2.10",
    "@types/history": "^4.7.7",
    "@types/jest": "^26.0.12",
    "@types/qs": "^6.9.4",
    "@types/react": "^16.9.49",
